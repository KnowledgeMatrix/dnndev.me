/****************END SCRIPT***********************************/

/***********DROP StdRegModule_DeleteStudent PROCEDURE *************/

IF EXISTS (select * FROM dbo.sysobjects WHERE id = object_id(N'{databaseOwner}[{objectQualifier}StdRegModule_DeleteStudent]') and OBJECTPROPERTY(id, N'IsProcedure') = 1)
    DROP PROCEDURE {databaseOwner}{objectQualifier}StdRegModule_DeleteStudent
GO

/****************END SCRIPT***********************************/

/***********DROP StdRegModule_DeleteStudent PROCEDURE *************/

IF EXISTS (select * FROM dbo.sysobjects WHERE id = object_id(N'{databaseOwner}[{objectQualifier}StdRegModule_getStudent]') and OBJECTPROPERTY(id, N'IsProcedure') = 1)
    DROP PROCEDURE {databaseOwner}{objectQualifier}StdRegModule_getStudent
