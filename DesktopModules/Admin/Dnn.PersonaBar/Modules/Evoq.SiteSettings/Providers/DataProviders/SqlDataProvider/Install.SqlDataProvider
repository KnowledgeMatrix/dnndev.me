UPDATE {databaseOwner}[{objectQualifier}PersonaBarMenu]
        SET Controller = 'Evoq.PersonaBar.SiteSettings.MenuControllers.SiteSettingsMenuController, Evoq.PersonaBar.SiteSettings'
        WHERE Identifier = 'Dnn.SiteSettings'
GO