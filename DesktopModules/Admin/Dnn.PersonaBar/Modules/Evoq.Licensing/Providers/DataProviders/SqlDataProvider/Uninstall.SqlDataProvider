UPDATE {databaseOwner}[{objectQualifier}PersonaBarMenu]
        SET Controller = 'Dnn.PersonaBar.Licensing.MenuControllers.HostMenuController, Dnn.PersonaBar.Licensing'
        WHERE Identifier = 'Dnn.Licensing'
GO