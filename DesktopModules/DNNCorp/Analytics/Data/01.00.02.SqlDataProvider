IF NOT EXISTS(
	SELECT ccu.COLUMN_NAME, ccu.CONSTRAINT_NAME
	FROM INFORMATION_SCHEMA.TABLE_CONSTRAINTS AS tc
	INNER JOIN INFORMATION_SCHEMA.CONSTRAINT_COLUMN_USAGE AS ccu ON tc.CONSTRAINT_NAME = ccu.CONSTRAINT_NAME
	WHERE tc.TABLE_NAME = '{objectQualifier}Analytics_Users' AND tc.CONSTRAINT_TYPE = 'PRIMARY KEY')
BEGIN
	ALTER TABLE {databaseOwner}[{objectQualifier}Analytics_Users] ADD CONSTRAINT PK_{objectQualifier}Analytics_Users PRIMARY KEY CLUSTERED (UserSampleId)
		WITH (STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF)
END
GO