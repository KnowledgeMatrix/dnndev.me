<script type="text/html" id="postCommentTemplate">
    <% if (Request.IsAuthenticated)
       { %>

    <li class="cmteditarea actStr_commentArea actStr_loadingContainer" data-bind="css: {loading: postArea.isSendingRequest}" style="overflow: visible; max-height: 0px; padding-top: 0px; padding-bottom: 0px;">
        <div contenteditable="false" data-bind="css: {showPlaceholder: postArea.showPlaceHolder, expanded: postArea.showContentEditable}">
            <div class="actStr_commentEditable" contenteditable="true" data-placeholder="<%= LocalizeSharedString("{resx:leavecomment}", true)%>" data-bind="editableEmojiHtml:postArea.postText"></div>
        </div>

        <div class="jcmtbuttons">
            <div class="actStr_postAreaTools" data-bind="with: postArea, css: {isPreviewerOpen: postArea.urlPreviewer.showPreviewer}">
                <% if (AllowFiles)
                   { %>
                <span class="actStr_addFileTool"  data-tooltip="<%= LocalizeString("ShareFile") %>" data-bind="click: toggleAttachFilePanel, css:{ selected: !fileUploader.isAttachingPhoto() && fileUploader.isAttachPanelOpen()}">
                    <i class="actStr_addFile"></i>
                </span>
                <% } %>
                <% if (AllowPhotos)
                   { %>
                <span class="actStr_attachPhoto"  data-tooltip="<%= LocalizeString("ShareImage") %>" data-bind="click: toggleAttachImagePanel, css:{ selected: fileUploader.isAttachingPhoto}">
                    <i class="actStr_addPhoto"></i>
                </span>
                <% } %>
                <% if (ShowEmojiPanel)
                   { %>
                <span class="actStr_emojiTool"  data-tooltip="<%= LocalizeString("Emoticon") %>" data-bind="click: showEmojiPanel, css:{ selected: isEmojiOpen}">
                    <i class="actStr_addEmoji"></i>
                </span>
                <% } %>
            </div>
            <a href="javascript:void(0)" class="dnnPrimaryAction" data-bind="click: postComment, css: {actStr_disable: postArea.disablePostButton}">
                <%= LocalizeSharedString("Reply") %>
            </a>
            <a class="dnnSecondaryAction" data-bind="click: clearComment">
                <%= LocalizeSharedString("Cancel") %>
            </a>
        </div>
        <!-- ko template: {name:'urlPreviewerTemplate', data: postArea.urlPreviewer} -->
        <!-- /ko -->
        <!-- ko template: {name:'fileUploadAreaTemplate', data: postArea.fileUploader, afterRender: postArea.fileUploader.bindFileUpload} -->
        <!-- /ko -->
    </li>
    <% } %>
</script>

<script type="text/html" id="urlPreviewerTemplate">
    <div class="jpa  actStr_urlPreviewer actStr_clearAfter" data-bind="visible: showPreviewer()" style="display: none">
        <span class="actStr_closePreviewBtn" data-bind="click: clearUrlPreview"></span>
        <div class="actStr_imagePreviewer" data-bind="visible: previewImageUrl() !== ''">
            <div class="actStr_imageWrapper">
                <img src="about:blank" alt="" data-bind="attr:{ alt: title, src: previewImageUrl}" />
            </div>
            <div class="actStr_imgPager" data-bind="visible: images().length > 1">
                <span class="pagerBtn dnnLeft" data-bind="click: prevImg">&lt;&lt;</span>
                <span class="pagerBtn dnnRight" data-bind="click: nextImg">&gt;&gt;</span>
                <span><span data-bind="text:currentImgIndex() + 1"></span><%= LocalizeString("Of.Text") %> <span data-bind="    text: images().length"></span></span>
            </div>
        </div>
        <div class="actStr_urlPreviewerInfo">
            <div class="actStr_urlTitle" data-bind="text: title"></div>
            <p data-bind="html: description"></p>
        </div>
    </div>
</script>

<script type="text/html" id="fileUploadAreaTemplate">
    <% if (AllowFiles || AllowPhotos)
       { %>
    <div class="fileUploadArea actStr_fileUploadArea actStr_clearAfter" data-bind="visibleAnimation: {condition:isAttachPanelOpen, effect: 'blind', options: {direction: 'vertical', duration: 200} }" style="display: none">
        <div class="jpa actStr_attachArea" data-bind="visibleAnimation: {condition:isAttachAreaOpen, effect: 'blind', options: {direction: 'vertical'}}">
            <div class="journal_onlineFileShare">
                <b data-bind="text:browseFromSiteText()"><%= LocalizeString("SelectPhoto.Text") %></b>
                <div>
                    <a href="javascript:void(0)" class="dnnSecondaryAction" data-bind="click: selectFromSite"><%= LocalizeString("BrowseFromSite.Text") %></a>
                    <a href="javascript:void(0)" class="actStr_openFromSite" style="display: none;"></a>
                </div>
            </div>
            <div class="journal_localFileShare">
                <b class="browser-upload-btn"><%= LocalizeString("UploadFromLocal.Text") %></b>
                <div class="dnnForm">
                    <input type="file" name="files[]" multiple />
                </div>
            </div>
            <div style="clear: both; padding: 0; margin: 0;"></div>
        </div>
        <div class="actStr_itemUpload dnnClear">
            <div class="fileupload-error dnnFormMessage dnnFormValidationSummary" style="display: none" data-bind="text: errorMessage, visibleAnimation: {condition:showErrorMessage, effect: 'blind'}"></div>
            <div class="progress_bar_wrapper">
                <div class="progress_context" style="display: none;" data-bind="visibleAnimation: {condition: showProgressBar, effect: 'fade'}">
                    <div class="progress-bar">
                        <div data-bind="text: progress, style:{width: progress}"><span></span></div>
                    </div>
                </div>
            </div>
            <div class="filePreviewArea" data-bind="visible:files().length>0">
                <!-- ko foreach: files -->
                <div data-bind="attr:{class: $parent.isAttachingPhoto() ? 'preview-photo-item' : 'tbar-attach-doc' }, visibleAnimation: {condition:true, effect: 'fade'}">
                    <img src="about:blank" data-bind="attr:{src: path}" />
                    <p data-bind="text:fileName, visible: !$parent.isAttachingPhoto()"></p>
                    <span class="actStr_closePreviewBtn" data-bind="click: $parent.removeAttached"></span>
                </div>
                <!-- /ko -->
                <div class="actStr_addMoreButtons" data-bind="visible: files().length < maxFiles">
                    <div data-bind="click: selectFromSite">
                        <img src="<%= ResolveUrl("~/DesktopModules/DNNCorp/ActivityStream/Images/add.png") %>" />
                        <%= LocalizeString("Browse") %>
                    </div>
                    <div class="actStr_upload" href="javascript:void(0)">
                        <img src="<%= ResolveUrl("~/DesktopModules/DNNCorp/ActivityStream/Images/add.png") %>" />
                        <%= LocalizeString("Upload") %>
                        <div class="dnnForm">
                            <input type="file" name="files[]" multiple />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <% } %>
</script>

<% } %>
