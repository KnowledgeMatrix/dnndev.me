<%@ Control Language="C#" Inherits="DotNetNuke.Professional.ActivityStream.View" AutoEventWireup="false" CodeBehind="View.ascx.cs" %>
<%@ Import Namespace="DotNetNuke.Professional.ActivityStream.Components.Common" %>
<%@ Register Namespace="DotNetNuke.Professional.ActivityStream.Controls" Assembly="DotNetNuke.Professional.ActivityStream" TagPrefix="dnn" %>
<%@ Register TagPrefix="social" TagName="CommentCard" Src="~/DesktopModules/DNNCorp/SocialLibrary/Controls/CommentCard.ascx" %>
<%@ Register TagPrefix="social" TagName="LikeControl" Src="~/DesktopModules/DNNCorp/SocialLibrary/Controls/LikeControl.ascx" %>
<%@ Register TagPrefix="social" TagName="GroupedJournalControl" Src="~/DesktopModules/DNNCorp/ActivityStream/GroupedJournalControl.ascx" %>
<div class="dnnClear">
	<asp:PlaceHolder ID="phUserControl" runat="server" />
</div>
<div id="userFileManager"></div>
<social:LikeControl runat="server" ID="ctlLikeControl" />
<social:CommentCard runat="server" ID="ctlCommentCard" />
<social:GroupedJournalControl runat="server" id="ctlGroupedJournalControl" />
<% if (!HideScopeWrapper)
   { %>
<div class="journalContainer" id="activityStream-ScopeWrapper">
    <% if (ShowEditor)
       { %>
    <div class="journalTools">
        <div id="avatar" runat="server" class="avatar">
            <span>
                <em>
                    <img src="<%= AvatarUrl %>" alt="avatar">
                </em>
            </span>
        </div>
        <div id="journalEditorArea" class="journalEditor-Container actStr_loadingContainer" data-bind="with: postStatusArea, css: {loading: postStatusArea.postArea.isSendingRequest}">
            <div id="journalEditor" class="actStr_clearAfter actStr_loadingContainer" data-bind="css: {actStr_expanded: isPostAreaOpen}">
                <div id="journalClose" data-bind="click: clearStatusPostArea"></div>
                <!-- ko with: postArea -->
                <div class="showPlaceholder" contenteditable="false" data-bind="css: {showPlaceholder: showPlaceHolder, expanded: showContentEditable}">
                    <div class="actStr_commentEditable" contenteditable="true" data-placeholder="<%= LocalizeString("SharePlaceHolder.Text") %>" data-bind="editableEmojiHtml:postText"></div>
                </div>
                <!-- /ko -->
                <div id="tbar" class="actStr_postAreaTools" data-bind="css: {isPreviewerOpen: postArea.urlPreviewer.showPreviewer}">

                    <span id="tbar-perm" class="actStr_permissionsTool" data-tooltip="<%= LocalizeString("ViewPermissions") %>" data-bind="click: toggleSecurityMenu, css:{selected: isSecurityMenuOpen}">
                        <i class="actStr_permissions"></i>
                    </span>
                    <!-- ko with: postArea -->
                    <% if (AllowFiles)
                       { %>
                    <span class="actStr_addFileTool" data-tooltip="<%= LocalizeString("ShareFile") %>" data-bind="click: toggleAttachFilePanel, css:{ selected: !fileUploader.isAttachingPhoto() && fileUploader.isAttachPanelOpen()}">
                        <i class="actStr_addFile"></i>
                    </span>
                    <% } %>
                    <% if (AllowPhotos)
                       { %>
                    <span class="actStr_attachPhoto" data-tooltip="<%= LocalizeString("ShareImage") %>" data-bind="click: toggleAttachImagePanel, css:{ selected: fileUploader.isAttachingPhoto}">
                        <i class="actStr_addPhoto"></i>
                    </span>
                    <% } %>
                    <% if (ShowEmojiPanel)
                       { %>
                    <span class="actStr_emojiTool" data-tooltip="<%= LocalizeString("Emoticon") %>" data-bind="click: showEmojiPanel, css:{ selected: isEmojiOpen}">
                        <i class="actStr_addEmoji"></i>
                    </span>
                    <% } %>
                    <!-- /ko -->
                    <!-- ko with: subscriptionSwitcher -->
                    <span id="tbar-subscribe" class="actStr_subscribeTool <%= IsSubscribedToWall? "selected" : "" %>" data-bind="click: toggleSubscription, css:{selected: isSubscribed}, attr: {'data-tooltip': altText}">
                        <i class="actStr_subscribe"></i>
                    </span>
                    <!-- /ko -->
                    <div class="securityMenu dnnClear" data-bind="visible: isSecurityMenuOpen" style="display: none">
                        <div class="handle" data-bind="click: toggleSecurityMenu"></div>
                        <ul>
                            <li><b><%= LocalizeString("WhoWillSee.Text") %></b></li>
                            <% if (!InGroupMode || IsPublicGroup)
                               { %>
                            <li>
                                <input type="radio" name="privacy" value="E" checked="<%= !InGroupMode ? "checked" : "" %>" /><%= LocalizeString("Everyone.Text") %></li>
                            <li>
                                <input type="radio" name="privacy" value="C" /><%= LocalizeString("Community.Text") %></li>
                            <% if (InGroupMode)
                               { %>
                            <li>
                                <input type="radio" name="privacy" value="R" checked="checked" /><%= LocalizeString("GroupMembers.Text") %></li>
                            <% } %>
                            <% }
                               else
                               { %>
                            <li>
                                <input type="radio" name="privacy" value="R" checked="checked" /><%= LocalizeString("GroupMembers.Text") %></li>
                            <% } %>
                            <% if (IsUserProfilePage)
                               { %>
                            <li>
                                <input type="radio" name="privacy" value="F" /><%= LocalizeString("Friends.Text") %></li>
                            <li>
                                <input type="radio" name="privacy" value="U" /><%= LocalizeString("Private.Text") %></li>
                            <% } %>
                        </ul>
                    </div>
                </div>
                <a href="javascript:void(0)" id="share" class="dnnPrimaryAction actStr_shareButton" data-bind="click: shareStatus, css: {actStr_disable: postArea.disablePostButton}"><%= LocalizeString("Share.Text") %></a>

            </div>
            <div id="journalOptionArea" data-bind="with: postArea">
                <!-- ko template: {name:'fileUploadAreaTemplate', data: fileUploader, afterRender: fileUploader.bindFileUpload} -->
                <!-- /ko -->
                <div data-bind="template: {name: 'urlPreviewerTemplate', data: urlPreviewer}"></div>
            </div>
        </div>
    </div>
    <% } %>
    <% if (Request.IsAuthenticated)
       { %>
    <div class="journalViewTools-container">
        <select id="selectActivityType" runat="server" class="select-view" data-bind="options: allowableFilterTypes, optionsText: 'FilterType', optionsValue: 'FilterTypeId', selectedOptions: selectedFilter, optionsCaption: selectActivityTypeCaption() "></select>
        <div class="dnnClear"></div>
    </div>
    <% } %>
    <div class="journalGetNewUpdates-container">
        <a href="javascript:void(0)" id="getNew" data-bind="click: getNewItems, visible: isGetNewButtonVisible"style="display: none"><%= LocalizeString("SeeNewUpdates") %></a>
    </div>
    <div id="journalItems" class=" <%= IsSubscribedToWall? "isSubscribedToWall" : "" %>" data-bind="css: {isSubscribedToWall : postStatusArea.subscriptionSwitcher.isSubscribed}, stopBindings: true">
        <dnn:JournalListControl ID="ctlJournalList" runat="server"></dnn:JournalListControl>
    </div>
    <div class="actStr_loadingContainer loading actStr_loadingMore" data-bind="visible: loadingMoreItems" style="display: none;"></div>
    <div class="actStr_noMoreItems" data-bind="visible: showNoMoreItemsToLoad" style="display: none;"><%= LocalizeString("NoMoreItemsToLoad") %></div>
    <div id="journalBottomControls" class="journalBottomContorls-container moduleTopInsetShadowBox">
        <a href="javascript:void(0)" style="display: none;" id="getMore" data-bind="click: getMoreItems, visible: isGetMoreButtonVisible" class="dnnPrimaryAction">
            <%= LocalizeString("GetMore") %>
        </a>
        <a href="javascript:void(0)" style="display: none;" id="showAll" data-bind="click: showAll, visible: isShowAllButtonVisible" class="dnnPrimaryAction">
            <%= LocalizeString("ShowAll") %>
        </a>
        <a href="<%= Links.Home(TabId) %>/view/rss" class="journal-social-rss"></a>
    </div>
</div>

